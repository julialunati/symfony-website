{% extends 'admin/base.html.twig' %}

{% block body %}

	<div class="container-fluid">
		<div class="row no-gutters justify-content-center mt-5">
			<div class="col-12 col-lg-6 col-md-6">
				<h1 class="display-4 text-center">
					{{ title }}
				</h1>
			</div>
		</div>
		<div class="row no-gutters justify-content-center mt-5">
			<ul>
				{% for user in users %}
					<li>{{ user.email }},
						{{ user.roles | json_encode }}</li>
				{% endfor %}
			</ul>
		</div>
	</div>


{% endblock %}
